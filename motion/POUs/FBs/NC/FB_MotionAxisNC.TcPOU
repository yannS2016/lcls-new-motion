  <POU Name="FB_MotionAxisNC" Id="{60e4ea75-36ff-4a14-b24a-51c7f7d1032f}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK FB_MotionAxisNC IMPLEMENTS I_MotionAxis
VAR_INPUT
END_VAR
VAR_OUTPUT
END_VAR
VAR
    iHome : I_Home;
    iHalt : I_Halt;
    iPower : I_Power;
    iReset : I_Reset;
    iMoveAbsolute : I_MoveAbsolute;
    iAxisParameters: I_AxisParameters;
    iAxisStatus : I_AxisStatus;
    iBacklashCompensation : I_BacklashCompensation;
    iMotionInterlocks:I_MotionInterlocks;
    bBusy : BOOL;
    bDone : BOOL;
    bError: BOOL;
    bAbortDone : BOOL;
    nErrorID: UDINT;
    sMessage : T_MAXSTRING;
    bExecute:BOOL;
    fPosition:LREAL;
    fVelocity:LREAL;
    fAcceleration:LREAL;
    fDeceleration:LREAL;
    fJerk:LREAL;
    eBufferMode:MC_BufferMode;
    bAxisEnabled : BOOL;
    eAxisState   : MC_AxisStates;
    bBacklashCompensationActive : BOOL;
    fBacklashCompensationRampVelo : LREAL;
    bExecHalt : BOOL;
    fHaltDeceleration : LREAL;
    fHaltJerk :LREAL;
    eHaltBufferMode : MC_BufferMode := MC_BufferMode.MC_Aborting;
    bExecHome : BOOL;
    fHomeSpeedFast:LREAL;
    fHomeSpeedSlow:LREAL;
    bHomeBusy : BOOL;
    bHomeDone : BOOL;
    eHomeMode : E_EpicsHomeCmd;
    bExecMoveAbsolute : BOOL;
bMoveAbsoluteAborted : BOOL;
fRefVeloSearch : LREAL;
fRefVeloSync : LREAL;
bExecReset : BOOL;
bResetDone : BOOL;
bMoveAbsoluteBusy : BOOL;
bStandStill : BOOL;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[bAxisEnabled:=THIS^.iAxisStatus.AxisEnabled();
eAxisState := THIS^.iAxisStatus.AxisState;
bHomeBusy := THIS^.iHome.HomeBusy;
bHomeDone := THIS^.iHome.HomeDone;
eHomeMode := THIS^.iHome.HomeMode;
bMoveAbsoluteAborted := THIS^.iMoveAbsolute.MoveAbsoluteAborted;
bMoveAbsoluteBusy := THIS^.iMoveAbsolute.MoveAbsoluteBusy;
fRefVeloSearch := THIS^.iAxisParameters.RefVeloSearch;
fRefVeloSync := THIS^.iAxisParameters.RefVeloSync;
THIS^.iReset.Reset(Execute:=bExecReset);
bResetDone := THIS^.iReset.ResetDone;
bStandStill := THIS^.iAxisStatus.StandStill;

THIS^.iHalt.Halt(	Execute:=bExecHalt,
                    Deceleration:=fHaltDeceleration,
                    Jerk:=fHaltJerk ,
                    BufferMode:=eHaltBufferMode
                );

// Forward motion params to MoveAbsolute block
THIS^.iMoveAbsolute.MoveAbsolute(	Execute:=bExecMoveAbsolute,
                                    Position:=fPosition,
                                    Velocity:=fVelocity,
                                    Acceleration:=fAcceleration,
                                    Deceleration:=fDeceleration,
                                    Jerk:=fJerk,
                                    BufferMode:=eBufferMode
                                );

THIS^.iHome.Home(	Execute:=bExecHome,
                    HomeSpeedFast:=fHomeSpeedFast,
                    HomeSpeedSlow:=fHomeSpeedSlow,
                );

THIS^.iBacklashCompensation.BacklashCompensationActive(	Active:=bBacklashCompensationActive,
                                                    RampVelocity:=fBacklashCompensationRampVelo );

// Get enable status for power block
// THIS^.PowerEnables(	Enable:=iMotionInterlocks.PowerEnabled,
//                     Enable_Positive:=iMotionInterlocks.MoveForwardEnabled,
//                     Enable_Negative:=iMotionInterlocks.MoveBackwardEnabled
//                   );
// // Home needs this for Homimg mode slection and update
// THIS^.iHome.HomeLimSw(  LimForward:=iMotionInterlocks.LimForward,
//                         LimBackward:=iMotionInterlocks.LimBackward,
//                         LimHome:=iMotionInterlocks.LimHome
//                     );
// // Lim status interlocks reacts to limits sw events
// THIS^.iMotionInterlocks.MotionStatus(
//                             HomeBusy:=THIS^.iHome.HomeBusy,
//                             PositiveDirectionEnabled:=THIS^.iAxisStatus.PositiveMotionIsEnabled,
//                             NegativeDirectionEnabled:=THIS^.iAxisStatus.NegativeMotionIsEnabled
//                         );
//
// THIS^.bDone := THIS^.iHome.HomeDone OR THIS^.iMoveAbsolute.MoveAbsoluteDone;
// THIS^.bBusy := THIS^.iHome.HomeBusy OR THIS^.iMoveAbsolute.MoveAbsoluteBusy;
// // Generalized abort event
// THIS^.bAbortDone := THIS^.iHalt.HaltDone OR THIS^.iReset.ResetDone;
// // Forward the abort event to homing block ( this is to sync the home state machine)
// THIS^.iHome.HomeAbort:=THIS^.bAbortDone;
//
// // handle combined Errors conditions
// THIS^.ErrorHandling();
//
]]></ST>
    </Implementation>
