    <Method Name="ErrorHandling" Id="{53078c0c-dd97-4634-ac7e-84052b41e871}">
      <Declaration><![CDATA[METHOD ErrorHandling
]]></Declaration>
      <Implementation>
        <ST><![CDATA[(*
    Error from functions and Nc
    The error will send to EPICS interface based on predifined
    priority: axis, power, backlash, absoluteMove, etc...
*)
IF THIS^.iAxisStatus.AxisError  THEN
    bError:=THIS^.iAxisStatus.AxisError;
    nErrorId:=THIS^.iAxisStatus.AxisErrorID;
ELSIF THIS^.iPower.PowerError AND THIS^.iPower.PowerIsEnabled THEN
    bError:=THIS^.iPower.PowerError;
    nErrorId:=THIS^.iPower.PowerErrorID;
ELSIF THIS^.iBacklashCompensation.BacklashCompError THEN
    bError:=THIS^.iBacklashCompensation.BacklashCompError;
    nErrorId:=THIS^.iBacklashCompensation.BacklashCompErrorID;
ELSIF THIS^.iMoveAbsolute.MoveAbsoluteError (*fbMcMoveAbsolute.Active*) THEN
    bError:=THIS^.iMoveAbsolute.MoveAbsoluteError;
    nErrorId:=THIS^.iMoveAbsolute.MoveAbsoluteErrorID;
ELSIF THIS^.iHome.HomeError (*Homing error*) THEN
    bError:=THIS^.iHome.HomeError;
    nErrorId:=THIS^.iHome.HomeErrorID;
ELSIF THIS^.iHalt.HaltError AND THIS^.iHalt.HaltActive THEN
    bError:=THIS^.iHalt.HaltError;
    nErrorId:=THIS^.iHalt.HaltErrorID;
ELSIF THIS^.iReset.ResetError  THEN
    bError:=THIS^.iReset.ResetError;
    nErrorId:=THIS^.iReset.ResetErrorID;
ELSIF THIS^.iAxisParameters.AxisParamsError THEN
    bError := THIS^.iAxisParameters.AxisParamsError;
    nErrorId := THIS^.iAxisParameters.AxisParamsErrorID;
ELSE
    bError:=FALSE;
    nErrorId:=0;
    sMessage := '';
END_IF;]]></ST>
      </Implementation>
    </Method>
